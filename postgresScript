CREATE DATABASE "apiDatabase"
    WITH 
    OWNER = postgres
    ENCODING = 'UTF8'
    LC_COLLATE = 'en_US.UTF-8'
    LC_CTYPE = 'en_US.UTF-8'
    TABLESPACE = pg_default
    CONNECTION LIMIT = -1;

CREATE TABLE Aluno (
	aluno_id integer,
	nome varchar(30) NOT NULL, 
	data_nascimento,
	cpf varchar(11) UNIQUE NOT NULL,
	nota integer NOT NULL
	PRIMARY KEY(aluno_id)
);